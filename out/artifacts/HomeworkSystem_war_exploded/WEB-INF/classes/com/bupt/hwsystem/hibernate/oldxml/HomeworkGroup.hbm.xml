<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.bupt.hwsystem.model.HomeworkGroup"
           table="homework_group">
        <id name="groupId" type="java.lang.Long">
            <column name="GROUP_ID"/>
            <generator class="identity"/>
        </id>
        <property name="homeworkId" type="java.lang.Long">
            <column name="HOMEWORK_ID" length="45" not-null="true"/>
        </property>
        <property name="submissionTime" type="java.util.Date">
            <column name="SUBMISSION_TIME" not-null="true"/>
        </property>
        <property name="score" type="java.lang.Integer">
            <column name="SCORE" not-null="false"/>
        </property>
        <property name="comment" type="string">
            <column name="COMMENT" not-null="false"/>
        </property>
        <property name="fileDir" type="string">
            <column name="FILEDIR" not-null="false"/>
        </property>
    </class>
</hibernate-mapping>